CREATE TABLE MetPlus
(INCHIKEY VARCHAR(30),
FORMULA VARCHAR(30),
MONOISOTOPIC_WEIGHTS REAL,
COMMON_NAME VARCHAR(100),
IUPAC_NAME VARCHAR(500),
SYSTEMATIC_NAME VARCHAR(500),
TRADE_NAME VARCHAR(500),
HMDB_ID VARCHAR(30),
LMDB_ID VARCHAR(30),
HUMANCYC_ID VARCHAR(20),
PUBCHEM_CID INT,
CHEMSPIDER_ID INT,
CHEBI_ID INT,
METLIN_ID INT,
KEGG_ID VARCHAR(30),
FooDB_ID VARCHAR(30));